#include <stdio.h>

void inputMatrix(int rows, int cols, int mat[rows][cols]) {
    printf("Enter elements of the matrix (%d x %d):\n", rows, cols);
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            scanf("%d", &mat[i][j]);
        }
    }
}

void multiplyMatrix(int r1, int c1, int A[r1][c1],
                    int r2, int c2, int B[r2][c2],
                    int result[r1][c2]) {

    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c2; j++) {
            result[i][j] = 0;  
            for (int k = 0; k < c1; k++) {
                result[i][j] += A[i][k] * B[k][j];
            }
        }
    }
}

void displayMatrix(int rows, int cols, int mat[rows][cols]) {
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            printf("%d ", mat[i][j]);
        }
        printf("\n");
    }
}

int main() {
    int r1 = 2, c1 = 2, r2 = 2, c2 = 2;

    int A[2][2], B[2][2], result[2][2];

    printf("Enter Matrix A:\n");
    inputMatrix(r1, c1, A);

    printf("Enter Matrix B:\n");
    inputMatrix(r2, c2, B);

    multiplyMatrix(r1, c1, A, r2, c2, B, result);

    printf("\nResult of Matrix Multiplication:\n");
    displayMatrix(r1, c2, result);

    return 0;
}
