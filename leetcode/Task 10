#include <stdio.h>

int removeDuplicates(int* nums, int size) {
    if (size == 0) return 0;
    int j = 0;
    for (int i = 1; i < size; i++) {
        if (nums[i] != nums[j]) {
            nums[++j] = nums[i];
        }
    }
    return j + 1;
}

int main() {
    int nums[] = {1,1,2,2,3};
    int len = removeDuplicates(nums, 5);
    for(int i = 0; i < len; i++) printf("%d ", nums[i]);
    return 0;
}
