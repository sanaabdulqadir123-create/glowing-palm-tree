#include <stdio.h>

int removeElement(int* nums, int size, int val) {
    int j = 0;
    for(int i = 0; i < size; i++) {
        if(nums[i] != val) nums[j++] = nums[i];
    }
    return j;
}

int main() {
    int nums[] = {3,2,2,3};
    int len = removeElement(nums, 4, 3);
    for(int i = 0; i < len; i++) printf("%d ", nums[i]);
    return 0;
}
