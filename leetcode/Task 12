#include <stdio.h>
#include <string.h>

void reverseString(char* s, int size) {
    for(int i = 0; i < size/2; i++) {
        char tmp = s[i];
        s[i] = s[size-i-1];
        s[size-i-1] = tmp;
    }
}

int main() {
    char s[] = "hello";
    int len = strlen(s);
    reverseString(s, len);
    printf("%s\n", s);
    return 0;
}
